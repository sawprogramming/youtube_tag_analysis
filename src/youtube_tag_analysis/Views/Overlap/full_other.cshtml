                                <year-select class="form-control" ng-model="vm.YearlyTitleDescrTag_Year"></year-select>
                            </div>
                        </div>
                    </div>
                    <div id="yearly_titledescrtag_histogram"></div>
                </div>
            </div>
        </div>
    </div>
</div>